    "jest": "^25.1.0",
    "lint-staged": "^10.0.2",
    "rimraf": "^3.0.2",
    "sucrase": "^3.12.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
